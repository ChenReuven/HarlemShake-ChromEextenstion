{
    "name": "Harlem Shake Extensions",
    "description": "Made By Chen Reuven",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "assets/hello_extensions.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            }
        }
    }
}